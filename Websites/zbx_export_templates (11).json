                            {
                                "type": "JAVASCRIPT",
                                "parameters": [
                                    "    value = parseFloat(value); // Convert the value to a float\n\n    if (value >= 100 && value <= 199) {\n        // Informational responses\n        return 2;\n    } else if (value >= 200 && value <= 299) {\n        // Successful responses\n        return 1;\n    } else if (value >= 300 && value <= 399) {\n        // Redirects\n        return 3;\n    } else if (value >= 405 && value <= 499) {\n        // Client errors\n        return 4;\n    } else if ((value >= 500 && value <= 599) || (value >= 400 && value <= 404)) {\n        // Server errors and specific range (400-404) for errors\n        return 0;\n    } else {\n        // Handle values outside the specified ranges if needed\n        return 1; // Or some other default value or error handling\n    }"
